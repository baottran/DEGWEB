        </div>
        <div class="form-group">
          <%= f.label :non_welded_suggested_action, 'Suggested Action', :class => "col-lg-3 form-label" %>
          <div class="col-lg-9">
            <%= f.text_field :non_welded_suggested_action, :class => "form-control" %>
          </div>
