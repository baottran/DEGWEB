          <div class="col-lg-9">
            <%= f.text_field :parts_oem_part_number, :class => "form-control" %>
          </div>
        </div>
        <div class="form-group">
          <%= f.label :parts_issue_summary, 'Issue Summary', :class => "col-lg-3 form-label" %>
          <div class="col-lg-9">
            <%= f.text_field :parts_issue_summary, :class => "form-control" %>
