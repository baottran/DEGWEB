          <div class="col-lg-9">
            <%= f.select(:welded_skill_level, skill_levels, {}, {class: 'form-control'}) %>
          </div>
        </div>
        <div class="form-group">
          <%= f.label 'Actual Time to Complete', :class => "col-lg-3 form-label" %>
