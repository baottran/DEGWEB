<% if logged_in? %>
<ul class="nav nav-pills">
    <li id="queue"><a href="/inquiries">Request Queue</a></li>
    <li id="new"><a href="/inquiries/new">New Inquiry</a></li>
    	<li id="reporting"><a href="/reporting">Reporting</a></li>
        <% if admin_logged_in? %>
        <li id="manageusers"><a href="/users">Manage Users</a></li>
        <li id="outofoffice"><a href="/out_of_office">Out of Office </a></li>
        <% end %>
    	<li id="logout"><a href="/logout">Log Out</a></li>
</ul>
<% end %>

<script>
<% if current === "queue" %>
	$("#queue").addClass("active");
<% elsif current === "reporting" %>
	$("#reporting").addClass("active");
<% elsif current === "new" %>
	$("#new").addClass("active");
<% elsif current === "users" %>
    $("#manageusers").addClass("active");
<% elsif current === "out_of_office" %>
    $("#outofoffice").addClass('active');
<% end %>
</script>

<style>
#logout {
    margin-left: 300px;
}
</style>


