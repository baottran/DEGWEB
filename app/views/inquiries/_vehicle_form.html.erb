<%= form_for @inquiry, url: {action: "edit_vehicle"}, html: {class: ""} do |f| %>
  <div class="form-field">
    <div class="form-horizontal">
      <div class="form-group">
        <div class="col-lg-3 form-label">
          <span class="form-required">* </span><%= f.label :make %>
        </div>
        <div class="col-lg-9">
          <%= f.select(:make, options_for_select([['<select>', ' '],
                                                  ['Acura', 'Acura'],
                                                  ['Alfa Romeo', 'Alfa Romeo'],
                                                  ['Aston Martin', 'Aston Martin'],
                                                  ['Audi', 'Audi'],
                                                  ['Bentley', 'Bentley'],
                                                  ['BMW', 'BMW'],
                                                  ['Buick', 'Buick'],
                                                  ['Cadillac', 'Cadillac'],
                                                  ['Chevrolet', 'Chevrolet'],
                                                  ['Chrysler', 'Chrysler'],
                                                  ['Dodge', 'Dodge'],
                                                  ['Ferrari', 'Ferrari'],
                                                  ['FIAT', 'FIAT'],
                                                  ['Ford', 'Ford'],
                                                  ['Freightliner', 'Freightliner'],
                                                  ['GMC', 'GMC'],
                                                  ['Honda', 'Honda'],
                                                  ['Hyundai', 'Hyundai'],
                                                  ['Infiniti', 'Infiniti'],
                                                  ['Jaguar', 'Jaguar'],
                                                  ['Jeep', 'Jeep'],
                                                  ['Kia', 'Kia'],
                                                  ['Lamborghini', 'Lamborghini'],
                                                  ['Land Rover', 'Land Rover'],
                                                  ['Lexus', 'Lexus'],
                                                  ['Lincoln', 'Lincoln'],
                                                  ['Maserati', 'Maserati'],
                                                  ['Mazda', 'Mazda'],
                                                  ['McLaren', 'McLaren'],
                                                  ['Mercedes-Benz', 'Mercedes-Benz'],
                                                  ['MINI', 'MINI'],
                                                  ['Mitsubishi', 'Mitsubishi'],
                                                  ['Nissan', 'Nissan'],
                                                  ['Porsche', 'Porsche'],
                                                  ['Ram', 'Ram'],
                                                  ['Rolls-Royce', 'Rolls-Royce'],
                                                  ['Scion', 'Scion'],
                                                  ['Smart', 'Smart'],
                                                  ['Subaru', 'Subaru'],
                                                  ['Tesla', 'Tesla'],
                                                  ['Toyota', 'Toyota'],
                                                  ['Volkswagen', 'Volkswagen'],
                                                  ['Volvo', 'Volvo'],
                                                  ['Other', 'Other']], @inquiry.make), {}, {class: 'form-control'}) %>
          <%= f.text_field :make_other_field, :class => "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-lg-3 form-label">
          <span class="form-required">* </span><%= f.label :model %>
        </div>
        <div class="col-lg-9">
          <%= f.text_field :model, :class => "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-lg-3 form-label">
          <span class="form-required">* </span><%= f.label :year %>
        </div>
        <div class="col-lg-9">
          <%= f.select(:year, options_for_select([['<select>', ' '],
                                                  ['2016', '2016'],
                                                  ['2015', '2015'],
                                                  ['2014', '2014'],
                                                  ['2013', '2013'],
                                                  ['2012', '2012'],
                                                  ['2011', '2011'],
                                                  ['2010', '2010'],
                                                  ['2009', '2009'],
                                                  ['2008', '2008'],
                                                  ['2007', '2007'],
                                                  ['2006', '2006'],
                                                  ['2005', '2005'],
                                                  ['2004', '2004'],
                                                  ['2003', '2003'],
                                                  ['2002', '2002'],
                                                  ['2001', '2001'],
                                                  ['2000', '2000'],
                                                  ['1999', '1999'],
                                                  ['1998', '1998'],
                                                  ['1997', '1997'],
                                                  ['1996', '1996'],
                                                  ['1995', '1995'],
                                                  ['1994', '1994'],
                                                  ['1993', '1993'],
                                                  ['1992', '1992'],
                                                  ['1991', '1991'],
                                                  ['1990', '1990'],
                                                  ['1989', '1989'],
                                                  ['1988', '1988'],
                                                  ['1987', '1987'],
                                                  ['1986', '1986']], @inquiry.year), {}, {class: 'form-control'}) %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :body_type, :class => "col-lg-3 form-label" %>
        <div class="col-lg-9">
          <%= f.select(:body_type, options_for_select([['<select>', ' '],
                                                       ['Sedan', 'Sedan'],
                                                       ['Crossover', 'Crossover'],
                                                       ['Pickup', 'Pickup'],
                                                       ['SUV', 'SUV'],
                                                       ['Coupe', 'Coupe'],
                                                       ['Luxury', 'Luxury'],
                                                       ['Hybrid', 'Hybrid'],
                                                       ['Van/Minivan', 'Van/Minivan'],
                                                       ['Convertible', 'Convertible'],
                                                       ['Hatchback', 'Hatchback'],
                                                       ['Wagon', 'Wagon'],
                                                       ['Electric', 'Electric']], @inquiry.body_type), {}, {class: 'form-control'}) %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-lg-3 form-label">
          <span class="form-required">* </span><%= f.label :vin %>
        </div>
        <div class="col-lg-9">
          <%= f.text_field :vin, :class => "form-control" %>
        </div>
      </div>
    </div>
	<%= f.submit "Save", class: "btn btn-primary btn-submit", html: { type: "submit", id: "vehicle-submit"} %>  
<% end %>