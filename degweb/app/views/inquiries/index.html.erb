<h1>Listing Inquiries</h1>

<%= link_to 'New Inquiry', new_inquiry_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>

  <% @inquiries.each do |inquiry| %>
    <tr>
      <td><%= inquiry.title %></td>
      <td><%= inquiry.text %></td>
      <td><%= link_to 'Show', inquiry_path(inquiry) %></td>
      <td><%= link_to 'Edit', edit_inquiry_path(inquiry) %></td>
      <td><%= link_to 'Delete', inquiry_path(inquiry),
        method: :delete,
        data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>
</table>